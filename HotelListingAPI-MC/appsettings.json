{
  "JwtSettings": {
    "Issuer": "HotelListingAPI-MC",
    "Audience": "HotelListingAPI-MCClient",
    "DurationInMinutes": 10
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://localhost:5341"
        }
      }
    ]
  },

  "AllowedHosts": "*"
}